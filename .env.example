# Harmoniq Environment Configuration
# Copy this file to .env and update values for your deployment

# Database
DB_USER=harmoniq
DB_PASSWORD=harmoniq_secret
DB_NAME=harmoniq

# JWT Secrets (CHANGE THESE IN PRODUCTION!)
JWT_SECRET=your-jwt-secret-change-me-min-32-chars
JWT_REFRESH_SECRET=your-refresh-secret-change-me-min-32-chars

# MinIO (S3-compatible storage)
MINIO_ACCESS_KEY=harmoniq
MINIO_SECRET_KEY=harmoniq_minio_secret_change_me

# TURN Server
COTURN_SECRET=harmoniq-turn-secret-change-me

# CORS
CORS_ORIGIN=https://your-domain.com

# TLS (for certbot)
DOMAIN=your-domain.com
EMAIL=admin@your-domain.com
